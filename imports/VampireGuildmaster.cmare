  endif
~</SCRIPT><TARGETMASK></TARGETMASK><FIZZLEMSG>&lt;S-NAME&gt; twitch(es) but fail(s) to change.</FIZZLEMSG><AUTOCASTMSG>&lt;T-NAME&gt; appears normal!</AUTOCASTMSG><CASTMSG></CASTMSG><POSTCASTMSG></POSTCASTMSG><ATTACKCODE>NOTHING</ATTACKCODE><POSTCASTAFFECT></POSTCASTAFFECT><POSTCASTABILITY></POSTCASTABILITY><POSTCASTDAMAGE>0</POSTCASTDAMAGE><HELP>&lt;ABILITY&gt;A vampire may end all of their Protean abilities early through the use of this ability.</HELP><TICKSBETWEENCASTS>0</TICKSBETWEENCASTS><TICKSOVERRIDE>1</TICKSOVERRIDE><TICKAFFECTS>false</TICKAFFECTS><CHANNELING>false</CHANNELING><UNINVOKEMSG></UNINVOKEMSG><MOCKABILITY></MOCKABILITY><MOCKABLETEXT></MOCKABLETEXT><TARGETFAILMSG></TARGETFAILMSG><NUMARGS>0</NUMARGS></ABILITY><ABILITY ID="Vampire_WolfClaws" TYPE="GenAbility"><CLASS>Vampire_WolfClaws</CLASS><NAME>Wolf Claws</NAME><DISPLAY>(Wolf Claws)</DISPLAY><TRIGSTR>WOLFCLAWS</TRIGSTR><MAXRANGE>MELEE</MAXRANGE><MINRANGE>MELEE</MINRANGE><AUTOINVOKE>false</AUTOINVOKE><FLAGS>0</FLAGS><CLASSIFICATION>SKILL,NOTHING</CLASSIFICATION><OVERRIDEMANA>-1</OVERRIDEMANA><USAGEMASK>HITPOINTS</USAGEMASK><CANAFFECTMASK>MOBS</CANAFFECTMASK><CANTARGETMASK>MOBS</CANTARGETMASK><QUALITY>OK_SELF</QUALITY><HERESTATS></HERESTATS><CASTMASK></CASTMASK><SCRIPT></SCRIPT><TARGETMASK></TARGETMASK><FIZZLEMSG>&lt;T-YOUPOSS&gt; fingernails fail to grow.</FIZZLEMSG><AUTOCASTMSG>&lt;T-NAME&gt; grow(s) powerful claws!</AUTOCASTMSG><CASTMSG>&lt;T-YOUPOSS&gt; fingernails grow into powerful claws.</CASTMSG><POSTCASTMSG></POSTCASTMSG><ATTACKCODE>NOTHING</ATTACKCODE><POSTCASTAFFECT></POSTCASTAFFECT><POSTCASTABILITY></POSTCASTABILITY><POSTCASTDAMAGE>0</POSTCASTDAMAGE><HELP>&lt;ABILITY&gt;You are able to grow an inch-long claw on each finger. These claws make excellent weapons in combat and can be used in many attacks. Wounds caused by the claws are especially vicious.</HELP><TICKSBETWEENCASTS>0</TICKSBETWEENCASTS><TICKSOVERRIDE>0</TICKSOVERRIDE><TICKAFFECTS>false</TICKAFFECTS><CHANNELING>false</CHANNELING><UNINVOKEMSG>&lt;T-YOUPOSS&gt; fingernails return to normal.</UNINVOKEMSG><MOCKABILITY>Spell_MageClaws</MOCKABILITY><MOCKABLETEXT></MOCKABLETEXT><TARGETFAILMSG></TARGETFAILMSG><NUMARGS>0</NUMARGS></ABILITY><ABILITY ID="Vampire_ShadowOfTheWolf" TYPE="GenAbility"><CLASS>Vampire_ShadowOfTheWolf</CLASS><NAME>Shadow of the Wolf</NAME><DISPLAY></DISPLAY><TRIGSTR>WOLFFORM</TRIGSTR><MAXRANGE>MELEE</MAXRANGE><MINRANGE>MELEE</MINRANGE><AUTOINVOKE>false</AUTOINVOKE><FLAGS>0</FLAGS><CLASSIFICATION>SKILL,NOTHING</CLASSIFICATION><OVERRIDEMANA>25</OVERRIDEMANA><USAGEMASK>HITPOINTS</USAGEMASK><CANAFFECTMASK>MOBS</CANAFFECTMASK><CANTARGETMASK>MOBS</CANTARGETMASK><QUALITY>OK_SELF</QUALITY><HERESTATS></HERESTATS><CASTMASK></CASTMASK><SCRIPT></SCRIPT><TARGETMASK></TARGETMASK><FIZZLEMSG>&lt;S-NAME&gt; attempt(s) to change forms, and fail(s).</FIZZLEMSG><AUTOCASTMSG></AUTOCASTMSG><CASTMSG></CASTMSG><POSTCASTMSG></POSTCASTMSG><ATTACKCODE>NOTHING</ATTACKCODE><POSTCASTAFFECT></POSTCASTAFFECT><POSTCASTABILITY></POSTCASTABILITY><POSTCASTDAMAGE>0</POSTCASTDAMAGE><HELP>&lt;ABILITY&gt;You may transform into a wolf. When in this form, you benefit from all of the obvious advantages, such as the wolf's ability to track.

The Revert Form ability is required to return to your normal form prior to the normal duration.</HELP><TICKSBETWEENCASTS>0</TICKSBETWEENCASTS><TICKSOVERRIDE>0</TICKSOVERRIDE><TICKAFFECTS>false</TICKAFFECTS><CHANNELING>false</CHANNELING><UNINVOKEMSG></UNINVOKEMSG><MOCKABILITY>Spell_PolymorphSelf</MOCKABILITY><MOCKABLETEXT>VampireWolf</MOCKABLETEXT><TARGETFAILMSG></TARGETFAILMSG><NUMARGS>0</NUMARGS></ABILITY><ABILITY ID="Vampire_Conditioning" TYPE="GenAbility"><CLASS>Vampire_Conditioning</CLASS><NAME>Conditioning</NAME><DISPLAY></DISPLAY><TRIGSTR>CONDITIONING</TRIGSTR><MAXRANGE>MELEE</MAXRANGE><MINRANGE>MELEE</MINRANGE><AUTOINVOKE>false</AUTOINVOKE><FLAGS>0</FLAGS><CLASSIFICATION>SKILL,NOTHING</CLASSIFICATION><OVERRIDEMANA>50</OVERRIDEMANA><USAGEMASK>HITPOINTS</USAGEMASK><CANAFFECTMASK>MOBS</CANAFFECTMASK><CANTARGETMASK>MOBS</CANTARGETMASK><QUALITY>MALICIOUS</QUALITY><HERESTATS>savemin +50</HERESTATS><CASTMASK></CASTMASK><SCRIPT>IMASK_PROG p Vampire_Conditioning
  if !hastattoo($t RecentlyDominated)
    mpsetvar $t vampiricdomination ++
    mpecho $t vampiric domination is set to $&lt;$t vampiricdomination&gt;.
  endif
  mpechoat $n You reinforce the willpower of $t.
  mpstop $t
  if var($t vampiricdomination &gt; 15) 
    if var ($t vampiricdomination &lt; 10000)
      mpstop $t
      if ispc($t)
        mpchoose $t WILLINGSERVANT YN N Do you accept $n as your master:
        if var($t WILLINGSERVANT == 'Y')
          mpforce $t SERVE $n
          mpsetvar $t vampiricdomination +10000
        else
          mpechoat $t You reject $n.
          mpechoat $n $t rejects you through a strong force of will.
          mptattoo $t '60 RecentlyDominated'
          mpsetvar $t vampiricdomination 0
        endif
      else
        mpforce $t SERVE $n
        mpunbehave antivagrant $t 
        mpunbehave aggressive $t
        mpunbehave alignhelper $t
        mpunbehave beggar $t
        mpunbehave brotherhelper $t
        mpunbehave clanhelper $t
        mpunbehave corpselooter $t
        mpunbehave doorwayguardian $t
        mpunbehave emoter $t
        mpunbehave faithhelper $t
        mpunbehave follower $t
        mpunbehave gateguard $t
        mpunbehave getsallequipped $t
        mpunbehave goldlooter $t 
        mpunbehave goodexecutioner $t
        mpunbehave goodguardian $t
        mpunbehave guard $t
        mpunbehave healer $t
        mpunbehave hireling $t
        mpunbehave mobhelper $t
        mpunbehave mobile $t
        mpunbehave mobileaggressive $t
        mpunbehave mobilegoodguardian $t
        mpunbehave mudchat $t
        mpunbehave nastyabilities $t
        mpunbehave neutralabilities $t
        mpunbehave niceabilities $t 
        mpunbehave objectguardian $t
        mpunbehave patroller $t
        mpunbehave playerhelper $t
        mpunbehave racehelper $t
        mpunbehave romgangmember $t
        mpunbehave rompatrolman $t
        mpunbehave scavenger $t
        mpunbehave vagrant $t
        mpunbehave veryaggressive $t
        mpunbehave wimpy $t
        mpunbehave wimpyaggressive $t
        mpsetvar $t vampiricdomination +10000
      endif
    else
      mpechoat $n $t has hardened their will against conditioning.
    endif
  else
    mpechoat $n The mind of $t has not been fully penetrated for your conditioning.
    mptattoo $t '60 RecentlyDominated'
  endif
~</SCRIPT><TARGETMASK>+RACECAT -Undead </TARGETMASK><FIZZLEMSG>&lt;S-NAME&gt; deeply probe(s) &lt;T-YOUPOSS&gt; mind, but fail(s) to breach &lt;T-HIS-HER&gt; mental defenses.</FIZZLEMSG><AUTOCASTMSG>&lt;T-NAME&gt; is mentally conditioned!</AUTOCASTMSG><CASTMSG>&lt;S-NAME&gt; deeply probe(s) &lt;T-YOUPOSS&gt; mind.</CASTMSG><POSTCASTMSG></POSTCASTMSG><ATTACKCODE>MIND</ATTACKCODE><POSTCASTAFFECT></POSTCASTAFFECT><POSTCASTABILITY></POSTCASTABILITY><POSTCASTDAMAGE>0</POSTCASTDAMAGE><HELP>&lt;ABILITY&gt;Over time, an individual can be Conditioned so that she is thereafter unable to resist your Domination and is also more resistant to the Domination of others. Kindred often Condition their servants to ensure loyal retainers. The procedure works based on the fact that mortals who have been Dominated repeatedly have a harder time resisting future attempts.

Conditioning is not something that is effective on the drop of a hat. It takes time to carefully cultivate the subject's mind (through use of the Command the Wearied Mind and Mesmerize abilities) for the kind of control you will exercise. Fully conditioning a subject can take weeks or even months.

Conditioned retainers have very little imagination, and tend to obey the orders given them quite literally. The conditioning actually destroys some of their free will, turning them into automatons.</HELP><TICKSBETWEENCASTS>0</TICKSBETWEENCASTS><TICKSOVERRIDE>1</TICKSOVERRIDE><TICKAFFECTS>false</TICKAFFECTS><CHANNELING>false</CHANNELING><UNINVOKEMSG></UNINVOKEMSG><MOCKABILITY></MOCKABILITY><MOCKABLETEXT></MOCKABLETEXT><TARGETFAILMSG>Undead and mindless creatures are not affected by this ability.</TARGETFAILMSG><NUMARGS>0</NUMARGS></ABILITY><ABILITY ID="Vampire_Telepathy" TYPE="GenAbility"><CLASS>Vampire_Telepathy</CLASS><NAME>Telepathic Scanning</NAME><DISPLAY>(Mind Reading)</DISPLAY><TRIGSTR>SCANMIND</TRIGSTR><MAXRANGE>MELEE</MAXRANGE><MINRANGE>MELEE</MINRANGE><AUTOINVOKE>false</AUTOINVOKE><FLAGS>0</FLAGS><CLASSIFICATION>SKILL,NOTHING</CLASSIFICATION><OVERRIDEMANA>-1</OVERRIDEMANA><USAGEMASK>MANA</USAGEMASK><CANAFFECTMASK>MOBS</CANAFFECTMASK><CANTARGETMASK>MOBS</CANTARGETMASK><QUALITY>OK_OTHERS</QUALITY><HERESTATS></HERESTATS><CASTMASK></CASTMASK><SCRIPT></SCRIPT><TARGETMASK></TARGETMASK><FIZZLEMSG>&lt;S-NAME&gt; attempt(s) to scan &lt;T-NAME&gt;, and fail(s).</FIZZLEMSG><AUTOCASTMSG>&lt;T-NAME&gt; &lt;T-IS-ARE&gt; heard!</AUTOCASTMSG><CASTMSG>&lt;S-NAME&gt; scan(s) &lt;T-NAME&gt;.</CASTMSG><POSTCASTMSG></POSTCASTMSG><ATTACKCODE>NOTHING</ATTACKCODE><POSTCASTAFFECT></POSTCASTAFFECT><POSTCASTABILITY>Spell_Telepathy</POSTCASTABILITY><POSTCASTDAMAGE>0</POSTCASTDAMAGE><HELP>&lt;ABILITY&gt;You can probe the surface thoughts of any nearby subject and listen to them as you would listen to that person speak. Given enough time, you can learn nearly anything about the subject. For instance, you might be able to divine the sire of another vampire through telepathic scanning.</HELP><TICKSBETWEENCASTS>0</TICKSBETWEENCASTS><TICKSOVERRIDE>0</TICKSOVERRIDE><TICKAFFECTS>false</TICKAFFECTS><CHANNELING>false</CHANNELING><UNINVOKEMSG></UNINVOKEMSG><MOCKABILITY>Thief_Evesdrop</MOCKABILITY><MOCKABLETEXT>@x1</MOCKABLETEXT><TARGETFAILMSG></TARGETFAILMSG><NUMARGS>1</NUMARGS></ABILITY><ABILITY ID="Vampire_ShadowOfTheRat" TYPE="GenAbility"><CLASS>Vampire_ShadowOfTheRat</CLASS><NAME>Shadow of the Rat</NAME><DISPLAY></DISPLAY><TRIGSTR>RATFORM</TRIGSTR><MAXRANGE>MELEE</MAXRANGE><MINRANGE>MELEE</MINRANGE><AUTOINVOKE>false</AUTOINVOKE><FLAGS>0</FLAGS><CLASSIFICATION>SKILL,NOTHING</CLASSIFICATION><OVERRIDEMANA>5</OVERRIDEMANA><USAGEMASK>HITPOINTS</USAGEMASK><CANAFFECTMASK>MOBS</CANAFFECTMASK><CANTARGETMASK>MOBS</CANTARGETMASK><QUALITY>OK_SELF</QUALITY><HERESTATS></HERESTATS><CASTMASK></CASTMASK><SCRIPT></SCRIPT><TARGETMASK></TARGETMASK><FIZZLEMSG>&lt;S-NAME&gt; attempt(s) to change forms, and fail(s).</FIZZLEMSG><AUTOCASTMSG></AUTOCASTMSG><CASTMSG></CASTMSG><POSTCASTMSG></POSTCASTMSG><ATTACKCODE>NOTHING</ATTACKCODE><POSTCASTAFFECT></POSTCASTAFFECT><POSTCASTABILITY></POSTCASTABILITY><POSTCASTDAMAGE>0</POSTCASTDAMAGE><HELP>&lt;ABILITY&gt;You may transform into a rat. When in this form, you benefit from all of the obvious advantages, such as the rat's small size.

The Revert Form ability is required to return to your normal form prior to the normal duration.</HELP><TICKSBETWEENCASTS>0</TICKSBETWEENCASTS><TICKSOVERRIDE>0</TICKSOVERRIDE><TICKAFFECTS>false</TICKAFFECTS><CHANNELING>false</CHANNELING><UNINVOKEMSG></UNINVOKEMSG><MOCKABILITY>Spell_PolymorphSelf</MOCKABILITY><MOCKABLETEXT>VampireRat</MOCKABLETEXT><TARGETFAILMSG></TARGETFAILMSG><NUMARGS>0</NUMARGS></ABILITY><ABILITY ID="Vampire_HeightenSenses" TYPE="GenAbility"><CLASS>Vampire_HeightenSenses</CLASS><NAME>Heighten Senses</NAME><DISPLAY>(Heightened Senses)</DISPLAY><TRIGSTR>HEIGHTENSENSES</TRIGSTR><MAXRANGE>MELEE</MAXRANGE><MINRANGE>MELEE</MINRANGE><AUTOINVOKE>false</AUTOINVOKE><FLAGS>0</FLAGS><CLASSIFICATION>SKILL,NOTHING</CLASSIFICATION><OVERRIDEMANA>10</OVERRIDEMANA><USAGEMASK>HITPOINTS</USAGEMASK><CANAFFECTMASK>MOBS</CANAFFECTMASK><CANTARGETMASK>MOBS</CANTARGETMASK><QUALITY>BENEFICIAL_SELF</QUALITY><HERESTATS></HERESTATS><CASTMASK></CASTMASK><SCRIPT>##The tattoos are to determine if the source of their affects are coming from this ability and should be removed when this ability is. Without the tattoos, they may inadvertently get removed from some other source.
IMASK_PROG p Vampire_HeightenSenses
  IF !AFFECTED($n Chant_Bloodhound)
    mpaffect property:Chant_Bloodhound $n
    mptattoo $n vampirebloodhounded
  ENDIF
  IF !AFFECTED($n Spell_Sonar)
    mptattoo $n vampiresonar
    mpaffect property:Spell_Sonar $n
  ENDIF
  IF EXPERTISE($n Vampire_HeightenSenses level &gt; 0)
    IF !AFFECTED($n Spell_DetectHidden)
      mptattoo $n vampiredetecthidden
      mpaffect property:Spell_DetectHidden $n
    ENDIF
    IF EXPERTISE($n Vampire_HeightenSenses level &gt; 1)
      IF !AFFECTED($n Spell_DetectAmbush)
        mptattoo $n vampiredetectambush
        mpaffect property:Spell_DetectAmbush $n
      ENDIF
      IF EXPERTISE($n Vampire_HeightenSenses level &gt; 3)
        IF !AFFECTED($n Spell_DetectInvisible)
          mptattoo $n vampiredetectinvisible
          mpaffect property:Spell_DetectInvisible $n
        ENDIF
        IF EXPERTISE($n Vampire_HeightenSenses level &gt; 5)
          IF !AFFECTED($n Spell_TrueSight)
            mptattoo $n vampiretruesight
            mpaffect property:Spell_TrueSight $n
          ENDIF
        ENDIF
      ENDIF
    ENDIF
  ENDIF
~

IMASK_PROG p UNINVOKE-Vampire_HeightenSenses
  IF AFFECTED($n Chant_Bloodhound)
    IF hastattoo($n vampirebloodhounded)
      mptattoo $n -vampirebloodhounded
      mpunaffect $n Chant_Bloodhound
    ENDIF
  ENDIF
  IF AFFECTED($n Spell_Sonar)
    IF hastattoo($n vampiresonar)
      mptattoo $n -vampiresonar
      mpunaffect $n Spell_Sonar
    ENDIF
  ENDIF
  IF AFFECTED($n Spell_DetectHidden)
    IF hastattoo($n vampiredetecthidden)
      mptattoo $n -vampiredetecthidden
      mpunaffect $n Spell_DetectHidden
    ENDIF
  ENDIF
  IF AFFECTED($n Spell_DetectAmbush)
    IF hastattoo($n vampiredetectambush)
      mptattoo $n -vampiredetectambush
      mpunaffect $n Spell_DetectAmbush
    ENDIF
  ENDIF
  IF AFFECTED($n Spell_DetectInvisible)
    IF hastattoo(mptattoo $n vampiredetectinvisible)
      mptattoo $n -vampiredetectinvisible
      mpunaffect $n Spell_DetectInvisible
    ENDIF
  ENDIF
  IF AFFECTED($n Spell_TrueSight)
