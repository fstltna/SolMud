  IF !hastattoo ($t BEASTHASBEENDRAWNOUT)
    IF rand0num (30 &lt; 28)
      mptattoo $t BEASTHASBEENDRAWNOUT
      if isbehave ($t Aggressive) or isbehave ($t VeryAggressive) or isbehave ($t WimpyAggressive) or isbehave ($t Wimpy)
      ##Don't change aggressiveness stance
      else
        IF rand0num (4 == 0)
          mpbehave WIMPY $t VERY=1 +NAMES -$n
          mptattoo $t DRAWNOUTBEASTWIMPY
        ELSE
          mpbehave Aggressive $t MOBKILL CHECKLEVEL +NAMES -$n
          mptattoo $t DRAWNOUTBEASTAGGRO
        ENDIF
      endif
    ENDIF
    IF rand0num (25 == 24)
      mpaffect $t chant_yearning
      mptattoo $t DRAWNOUTBEASTYEARNING
    ENDIF
  ENDIF
~

