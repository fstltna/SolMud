        else

          mpsetvar $t VAR_NAME_NEW $&lt;$t VAR_NAME_NEW&gt; $%GSTAT($t name)%.$0

        endif

      endif

    endif 

  next

  mpgset $t name $&lt;$t VAR_NAME_NEW&gt;


