    else

      if eval('$%GSTAT($t name)%.$0' == $g.0)

        mpsetvar $t VAR_NAME_NEW $&lt;$t VAR_NAME_NEW&gt; $g.1

      else

        if eval('$%GSTAT($t name)%.$0' == 'a')

