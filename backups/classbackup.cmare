    else
mpbehave CombatAbilities $t MINTICKs=2
    endif
    mpcallfunc AssignSkeletalArcherAbilities
    mptattoo $t UNDEADJOB
    mpechoat $n &lt;T-NAME&gt; salute(s) &lt;S-NAME&gt; and look(s) for a ranged weapon.
  else
    mpechoat $n Only certain types of undead may be affected by this prayer.
  endif
else
  mpechoat $n $t already has an assigned role.
endif
else
mpechoat $n Only certain forms of undead may be affected by this prayer!
endif
~
function_prog AssignSkeletalArcherAbilities
mpenable $t specialization_ranged 100
mpenable $t spell_conjureammunition 100
mpgset $t description $%GSTAT($t description)% This skeleton is a master marksman.
mpforce $t automelee off
mpenable $t Fighter_RapidShot 90
mpenable $t Fighter_FarShot 90
mpenable $t Fighter_PointBlank 90
mpenable $t Fighter_CalledShot 90
mpenable $t Fighter_CriticalShot 90
mpenable $t Fighter_TrueShot 90
