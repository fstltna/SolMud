function_prog AssignMummyMageAbilities 
mpenable $t Spell_MagicMissile 100 
mpenable $t Spell_ResistMagicMissiles 100 
mpenable $t Spell_MinorGlobe 100 
mpforce automelee off 
switch $%RANDNUM(5)% 

case 1  
